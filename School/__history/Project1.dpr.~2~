program Project1;

uses
  Vcl.Forms,
  MainUN in 'MainUN.pas' {Form1},
  CreateClassUN in 'CreateClassUN.pas' {FmCreateClass},
  CreateTeacherUN in 'CreateTeacherUN.pas' {Form3},
  CreateStudentUN in 'CreateStudentUN.pas' {Form2},
  StudentandTeacherUN in 'StudentandTeacherUN.pas' {Form4},
  StudentandClassUN in 'StudentandClassUN.pas' {Form5};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TFmCreateClass, FmCreateClass);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TForm4, Form4);
  Application.CreateForm(TForm5, Form5);
  Application.Run;
end.
